<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/navigation_bar_settings" >

    <ListPreference
        android:dialogTitle="@string/navigation_bar_height"
        android:entries="@array/navigation_bar_height_entries"
        android:entryValues="@array/navigation_bar_height_values"
        android:key="navigation_bar_height"
        android:persistent="false"
        android:title="@string/navigation_bar_height" />

    <SwitchPreference
        android:key="navigation_swap_back_recents"
        android:summary="@string/navigation_swap_back_recents_summary"
        android:title="@string/navigation_swap_back_recents_title" />

</PreferenceScreen>